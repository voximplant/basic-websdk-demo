{
  "name": "basic-websdk-demo",
  "version": "1.0.0",
  "scripts": {
    "commitlint": "commitlint",
    "lint": "standardx --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "yarn commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn lint"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "standardx": "^7.0.0"
  }
}

{
  "name": "basic-websdk-demo",
  "version": "1.0.1",
  "scripts": {
    "commitlint": "commitlint",
    "lint": "standardx --fix",
    "prettier": "prettier --write ./src"
  },
  "author": "Zingaya Inc. <info@voximplant.com> (https://voximplant.com)",
  "repository": {
    "type": "git",
    "url": "git@git.zingaya.com:voximplant-core/websdk.git"
  },
  "tags": [
    "demo",
    "client-sdk",
    "voximplant"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "yarn commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn lint", "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "husky": "4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.3.0",
    "standardx": "^7.0.0"
  }
}
